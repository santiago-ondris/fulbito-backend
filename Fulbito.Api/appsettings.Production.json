{
    "Logging": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "DefaultConnection": "${DATABASE_URL}"
    },
    "Jwt": {
      "Key": "${JWT_SECRET_KEY}",
      "Issuer": "FulbitoApi",
      "Audience": "FulbitoApp"
    },
    "Cloudinary": {
      "CloudName": "${CLOUDINARY_CLOUD_NAME}",
      "ApiKey": "${CLOUDINARY_API_KEY}",
      "ApiSecret": "${CLOUDINARY_API_SECRET}"
    }
  }